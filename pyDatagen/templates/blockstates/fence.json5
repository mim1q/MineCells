{
  "multipart": [
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_fence_post"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_fence_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_fence_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_fence_side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_fence_side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    }
  ]
}