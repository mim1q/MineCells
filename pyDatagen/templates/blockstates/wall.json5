{
  "multipart": [
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_post"
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side",
        "uvlock": true
      },
      "when": {
        "north": "low"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "low"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "low"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "low"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side_tall",
        "uvlock": true
      },
      "when": {
        "north": "tall"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side_tall",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "tall"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side_tall",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "tall"
      }
    },
    {
      "apply": {
        "model": "{{id}}:block/{{base}}_wall_side_tall",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "tall"
      }
    }
  ]
}